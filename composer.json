{
  "name": "divante-ltd/pimcore-scheduled-export",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Scheduled Export lets you run ordinary grid exports in background.",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://www.divante.co/",
  "authors": [
    {
      "name": "Anna Zavodian",
      "email": "azavodian@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Monika Litwin",
      "email": "mlitwin@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Jakub PÅ‚askonka",
      "email": "jplaskonka@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Wojciech Peisert",
      "email": "wpeisert@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Bruno Ramalho",
      "email": "bramalho@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Michael Frank",
      "email": "mf@the-spirit-of-it.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "vendor-dir": "vendor",
    "bin-dir": "vendor/bin/",
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "disable-tls": false,
    "secure-http": true,
    "allow-plugins": {
      "ocramius/package-versions": true
    }
  },
  "require": {
    "ext-json": "*",
    "php": "^8.0",
    "pimcore/pimcore": "^10.0",
    "elements/process-manager-bundle": "^4.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "divante-ltd/pimcore-coding-standards": "^0.1",
    "phploc/phploc": "^7.0",
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Divante\\ScheduledExportBundle\\": "src/ScheduledExportBundle"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Divante\\ScheduledExportBundle\\DivanteScheduledExportBundle"
      ]
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
